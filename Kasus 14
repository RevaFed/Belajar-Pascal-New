program pangkat;
        uses crt;
                var
                a,b, counter, hasil : integer;
        begin
                clrscr;
                        hasil:=1;
                                write('Input bilangan pertama : '); readln(a);
                                write('Input Bilangn kedua : '); readln(b);

                                        for counter := 1 to b do
                                                begin
                                                        hasil := hasil * a;
                                                end;
                                        writeln('Maka nilai ', a, ' angkat ', b, ' adalah : ', hasil);

                        readln;
        end.
